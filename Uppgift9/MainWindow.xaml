        <Button x:Name="btnKontrollera" Content="Kontrollera" HorizontalAlignment="Left" Margin="413,217,0,0" VerticalAlignment="Top" Height="23" Width="61" Click="btnKontrollera_Click"/>
        <Label x:Name="lblResultat" HorizontalAlignment="Left" Margin="263,262,0,0" VerticalAlignment="Top" Height="60" Width="537"/>
